class Solution {
    public int numJewelsInStones(String J, String S) {
        int count = 0;
        int n1,n2;
        n1 = J.length();
        n2 = S.length();
        int i,j;
        for(i=0;i<n2;i++){
            for(j=0;j<n1;j++){
                if(S.charAt(i)==J.charAt(j))
                    count++;
            }    
        }
        return count;
    }
}
